    <link rel="stylesheet" href="https://rsms.me/inter/inter.css">
    <%- include('webpack.ejs') %>
    <script src="https://cdn.jsdelivr.net/gh/alpinejs/alpine@v2.7.3/dist/alpine.min.js"></script>
    <script>
      function dropdown() {
        return {
          show: false,
          open() { this.show = true },
          close() { this.show = false },
          toggle() { this.show = !this.show },
          isOpen() { return this.show === true },
        }
      }
  </script>
  </head>

  <body class="antialiased text-gray-900">

    <div x-data="{ showMobileMenu: false }" class="relative bg-white z-20">
      <div class="max-w-7xl mx-auto px-4 sm:px-6">
        <div class="flex justify-between items-center py-6 md:justify-start md:space-x-10">
          <div class="flex justify-start lg:w-0 lg:flex-1">
            <a href="/">
              <span class="sr-only">Archboard</span>
              <img class="h-8 w-auto sm:h-10" src="/assets/images/full-color.svg" alt="Archboard">
            </a>
          </div>

          <!-- menu button -->
          <div class="-mr-2 -my-2 md:hidden">
            <button x-on:click.prevent="showMobileMenu = !showMobileMenu" type="button" class="bg-white rounded-md p-2 inline-flex items-center justify-center text-gray-400 hover:text-gray-500 hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-blue-500">
              <span class="sr-only">Open menu</span>
              <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
              </svg>
            </button>
          </div>

          <!-- desktop navigation -->
