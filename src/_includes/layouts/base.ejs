            <div x-data="dropdown()" class="relative">
              <!-- Item active: "text-gray-900", Item inactive: "text-gray-500" -->
              <button x-on:click.prevent="toggle" type="button" class="group bg-white rounded-md text-gray-500 inline-flex items-center text-base font-medium hover:text-gray-900 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500">
